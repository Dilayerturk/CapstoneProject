@model refactoradiyos_final.Models.GeneralModel

@{
    ViewBag.Title = "index";
    Layout = null;
}

<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="description" content="A fully featured admin theme which can be used to build CRM, CMS, etc.">
    <meta name="author" content="dilayerturk">

    <link rel="shortcut icon" href="assets/images/favicon.ico">

    <title>Adiyos - Smart Diabetes Mellitus Management Tool</title>

    <link href="~/plugins/switchery/switchery.min.css" rel="stylesheet" />
    <link href="~/plugins/jquery-circliful/css/jquery.circliful.css" rel="stylesheet" type="text/css" />

    <link href="~/assets/css/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link href="~/assets/css/icons.css" rel="stylesheet" type="text/css">
    <link href="~/assets/css/style.css" rel="stylesheet" type="text/css">

    <script src="~/assets/js/modernizr.min.js"></script>
    <style>
        @@media (min-width: 768px) {
            .modal-xl {
                width: 90%;
                max-width: 1200px;
            }
        }
    </style>


</head>

<body class="fixed-left">

    @using (Html.BeginForm("create", "KayitFormuEdit", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <!-- Begin page -->
        <div id="wrapper">


            <!-- Top Bar Start -->
            <div class="topbar">

                <!-- LOGO -->
                <div class="topbar-left">
                    <div class="text-center">
                        <a href="index.html" class="logo"><i class="mdi mdi-radar"></i> <span>ADİYOS</span></a>
                    </div>
                </div>

                <!-- Button mobile view to collapse sidebar menu -->
                <nav class="navbar-custom">

                    <ul class="list-inline float-right mb-0">

                        <li class="list-inline-item notification-list hide-phone">
                            <a class="nav-link waves-light waves-effect" href="#" id="btn-fullscreen">
                                <i class="mdi mdi-crop-free noti-icon"></i>
                            </a>
                        </li>

                        <li class="list-inline-item notification-list">
                            <a class="nav-link right-bar-toggle waves-light waves-effect" href="#">
                                <i class="mdi mdi-dots-horizontal noti-icon"></i>
                            </a>
                        </li>

                        <li class="list-inline-item dropdown notification-list">
                            <a class="nav-link dropdown-toggle arrow-none waves-light waves-effect" data-toggle="dropdown"
                               href="#" role="button" aria-haspopup="false" aria-expanded="false">
                                <i class="mdi mdi-bell noti-icon"></i>
                                <span class="badge badge-pink noti-icon-badge">4</span>
                            </a>
                            <div class="dropdown-menu dropdown-menu-right dropdown-arrow dropdown-menu-lg"
                                 aria-labelledby="Preview">
                                <!-- item-->
                                <div class="dropdown-item noti-title">
                                    <h5 class="font-16">
                                        <span class="badge badge-danger float-right">5</span>Notification
                                    </h5>
                                </div>

                                <!-- item-->
                                <a href="javascript:void(0);" class="dropdown-item notify-item">
                                    <div class="notify-icon bg-success"><i class="mdi mdi-comment-account"></i></div>
                                    <p class="notify-details">
                                        Robert S. Taylor commented on Document<small class="text-muted">
                                            1
                                            min ago
                                        </small>
                                    </p>
                                </a>

                                <!-- item-->
                                <a href="javascript:void(0);" class="dropdown-item notify-item">
                                    <div class="notify-icon bg-info"><i class="mdi mdi-account"></i></div>
                                    <p class="notify-details">
                                        New user registered.<small class="text-muted">
                                            1 min
                                            ago
                                        </small>
                                    </p>
                                </a>

                                <!-- item-->
                                <a href="javascript:void(0);" class="dropdown-item notify-item">
                                    <div class="notify-icon bg-danger"><i class="mdi mdi-airplane"></i></div>
                                    <p class="notify-details">
                                        Carlos Crouch liked <b>Brief</b><small class="text-muted">
                                            1
                                            min ago
                                        </small>
                                    </p>
                                </a>

                                <!-- All-->
                                <a href="javascript:void(0);" class="dropdown-item notify-item notify-all">
                                    View All
                                </a>

                            </div>
                        </li>

                        <li class="list-inline-item dropdown notification-list">
                            <a class="nav-link dropdown-toggle waves-effect waves-light nav-user" data-toggle="dropdown"
                               href="#" role="button" aria-haspopup="false" aria-expanded="false">
                                <img src="~/assets/images/users/avatar-1.jpg" alt="user" class="rounded-circle">
                            </a>
                            <div class="dropdown-menu dropdown-menu-right profile-dropdown " aria-labelledby="Preview">
                                <!-- item-->
                                <div class="dropdown-item noti-title">
                                    <h5 class="text-overflow"><small>Hoşgeldin ! </small> </h5>
                                </div>

                                <!-- item-->
                                <a href="extras-profile.html" class="dropdown-item notify-item">
                                    <i class="mdi mdi-account"></i> <span>Profil</span>
                                </a>


                                <!-- item-->
                                <a href="@Url.Action("Logout","User")" class="dropdown-item notify-item">
                                    <i class="mdi mdi-logout"></i> <span>Çıkış</span>
                                </a>

                            </div>
                        </li>

                    </ul>

                    <ul class="list-inline menu-left mb-0">
                        <li class="float-left">
                            <button class="button-menu-mobile open-left waves-light waves-effect">
                                <i class="mdi mdi-menu"></i>
                            </button>
                        </li>

                    </ul>

                </nav>

            </div>
            <!-- Top Bar End -->
            <!-- ========== Left Sidebar Start ========== -->

            <div class="left side-menu">
                <div class="sidebar-inner slimscrollleft">
                    <!--- Divider -->
                    <div id="sidebar-menu">
                        <ul>
                            <li class="menu-title">Genel</li>
                            <li>
                                <a href="~/FirstPage/index" class="waves-effect waves-primary">
                                    <i class="ti-home"></i><span>Anasayfa</span>
                                </a>
                            </li>

                            <li class="has_sub">
                                <a href="javascript:void(0);" class="waves-effect waves-primary">
                                    <i class="ti-email"></i><span>Dilekçe İşlemleri</span>
                                    <span class="menu-arrow"></span>
                                </a>
                                <ul class="list-unstyled">
                                    <li>@Html.ActionLink("Gelen Kutusu", "inbox", "MailCompose")</li>
                                    <li>@Html.ActionLink("E-posta Gönder", "index", "MailCompose")</li>
                                </ul>
                            </li>
                            <li class="has_sub">
                                <a href="javascript:void(0);" class="waves-effect waves-primary">
                                    <i class="ti-pencil-alt"></i><span>Hasta İşlemleri</span>
                                    <span class="menu-arrow"></span>
                                </a>
                                <ul class="list-unstyled">
                                    <li>@Html.ActionLink("Kayıt Formu Ekle", "index", "KayitFormu")</li>
                                    <li>@Html.ActionLink("Hasta Listesi", "index", "TablesEditable")</li>
                                </ul>
                            </li>
                            <li class="menu-title">Daha Fazla</li>
                            <li>
                                <a href="~/AppsTeam/index" class="waves-effect waves-primary">
                                    <i class="ti-user"></i><span>Doktorlar</span>
                                </a>
                            </li>

                            <li class="has_sub">
                                <a href="javascript:void(0);" class="waves-effect waves-primary">
                                    <i class="ti-stats-up"></i><span>Analiz Tabloları</span>
                                    <span class="menu-arrow"></span>
                                </a>
                                <ul class="list-unstyled">
                                    <li>@Html.ActionLink("Genel Analiz", "index", "ChartChartlist")</li>
                                </ul>
                            </li>
                        </ul>
                        <div class="clearfix"></div>
                    </div>
                    <div class="clearfix"></div>
                </div>
            </div>
            <!-- Left Sidebar End -->
            <!-- Right Sidebar -->
            <div class="content-page">
                <!-- Start content -->
                <div class="content">
                    <div class="container-fluid">

                        <!-- Page-Title -->
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="page-title-box">
                                    <h4 class="text-dark page-title">HASTA FORMU DÜZENLEME</h4>
                                    <div class="clearfix"></div>
                                </div>
                            </div>
                        </div>

                        <p class="text-muted m-b-25 font-13">
                            Hasta kayıt formu daha önce sisteme kayıt olmuş ve hasta
                            bilgilerini düzenlemek için oluşturulmuştur.
                        </p>

                        <h5 style="color:red">@Html.ViewData["Message"] </h5>
                        <h5 style="color:green">@Html.ViewData["MessageSuccess"] </h5>

                        <div class="row">
                            <div class="col-12">
                                <div class="card-box">
                                    <div class="row">
                                        <div class="col-sm-6">
                                            <div class="card m-b-20 card-body">
                                                <h4 class="text-dark card-title">Hasta Muayene Formu</h4>
                                                <p class="card-text">
                                                    Hastanın genel bilgileri, diyabet bilgileri, kullandığı
                                                    ilaçlar ve medikal geçmiş verilerinin bulunduğu formdur.
                                                </p>
                                                <button type="button"
                                                        class="btn btn-primary waves-effect waves-light btn-block"
                                                        data-toggle="modal" data-target="#con-close-modal-1">
                                                    DÜZENLE
                                                </button>
                                            </div>
                                        </div>
                                        <div class="col-sm-6">
                                            <div class="card m-b-20 card-body">
                                                <h4 class="text-dark card-title">Fizik Muayene Formu</h4>
                                                <p class="card-text">
                                                    Endokrin sistem, Kardiyovasküler sistem, Nörolojik
                                                    sistem ve komplikasyon verilerinin bulunduğu formdur.
                                                </p>
                                                <button type="button"
                                                        class="btn btn-primary waves-effect waves-light btn-block"
                                                        data-toggle="modal" data-target="#con-close-modal-2">
                                                    DÜZENLE
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- end row -->

                                    <div class="row">
                                        <div class="col-sm-4 col-xs-12">
                                            <div class="card m-b-20 card-body">
                                                <h4 class="text-dark card-title">Laboratuvar İnceleme Formu</h4>
                                                <p class="card-text">
                                                    Hemoglobin, idrar ve biyokimya verilerinin bulunduğu
                                                    formdur.
                                                </p>
                                                <button type="button"
                                                        class="btn btn-primary waves-effect waves-light btn-block"
                                                        data-toggle="modal" data-target="#con-close-modal-3">
                                                    DÜZENLE
                                                </button>
                                            </div>
                                        </div>

                                        <div class="col-sm-4 col-xs-12">
                                            <div class="card m-b-20 card-body text-xs-center">
                                                <h4 class="text-dark card-title">Değerlendirme, Öneri ve Tedavi Formu</h4>
                                                <p class="card-text">
                                                    Tanılar, önerilen tedavi ve konsultasyon verilerinin
                                                    bulunduğu formdur.
                                                </p>
                                                <button type="button"
                                                        class="btn btn-primary waves-effect waves-light btn-block"
                                                        data-toggle="modal" data-target="#con-close-modal-4">
                                                    DÜZENLE
                                                </button>
                                            </div>
                                        </div>

                                        <div class="col-sm-4 col-xs-12">
                                            <div class="card m-b-20 card-body text-xs-right">
                                                <h4 class="text-dark card-title">Tetkik Formu</h4>
                                                <p class="card-text">İstenen tetkik verilerinin bulunduğu formdur.</p>
                                                <button type="button"
                                                        class="btn btn-primary waves-effect waves-light btn-block"
                                                        data-toggle="modal" data-target="#con-close-modal-5">
                                                    DÜZENLE
                                                </button>
                                            </div>
                                        </div>

                                        <div class="row">
                                            <div class="card-box">
                                                <div class="text-right">
                                                    

                                                    <button type="submit"
                                                            class="btn btn-success btn-custom waves-effect w-md waves-light m-b-5">
                                                        Kaydet
                                                    </button>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                    <!-- end row -->
                                    <!-- Modal 1 Start-->



                                    <div id="con-close-modal-1" class="modal fade" tabindex="-1" role="dialog"
                                         aria-labelledby="myModalLabel" aria-hidden="true" style="display: none;">
                                        <div class="modal-dialog modal-xl">
                                            <div class="modal-content">
                                                <div class="modal-header">
                                                    <button type="button" class="close" data-dismiss="modal"
                                                            aria-hidden="true">
                                                        ×
                                                    </button>
                                                    <h4 class="modal-title">Hasta Muayene Formu</h4>
                                                </div>
                                                <div class="modal-body">
                                                    <div class="row">
                                                        <div class="col-md-2">
                                                            <div class="form-group">
                                                                <label for="field-0" class="control-label">
                                                                    Başvuru
                                                                    Tarihi
                                                                </label>
                                                                @Html.TextBoxFor(m => m.patient_Info.AppDate, new { @class = "form-control", type = "date", name = "date" })

                                                                @Html.HiddenFor(m => m.patient_Info.ID)
                                                                @Html.HiddenFor(m => m.biochemistry.ID)
                                                                @Html.HiddenFor(m => m.bos.ID)
                                                                @Html.HiddenFor(m => m.c24.ID)
                                                                @Html.HiddenFor(m => m.cardiovascular_System.ID)
                                                                @Html.HiddenFor(m => m.clinic_Chemistry.ID)
                                                                @Html.HiddenFor(m => m.coagulation.ID)
                                                                @Html.HiddenFor(m => m.complication.ID)
                                                                @Html.HiddenFor(m => m.consultation.ID)
                                                                @Html.HiddenFor(m => m.decision_After_Evaluation.ID)
                                                                @Html.HiddenFor(m => m.diabetes_Education.ID)
                                                                @Html.HiddenFor(m => m.diabetes_Info.ID)
                                                                @Html.HiddenFor(m => m.drug_Layouts.ID)
                                                                @Html.HiddenFor(m => m.endocrine.ID)
                                                                @Html.HiddenFor(m => m.habit.ID)
                                                                @Html.HiddenFor(m => m.hematoloji.ID)
                                                                @Html.HiddenFor(m => m.hemoglobin.ID)
                                                                @Html.HiddenFor(m => m.history_Family.ID)
                                                                @Html.HiddenFor(m => m.hormone.ID)
                                                                @Html.HiddenFor(m => m.isletAntibody.ID)
                                                                @Html.HiddenFor(m => m.medical_History.ID)
                                                                @Html.HiddenFor(m => m.medicine.ID)
                                                                @Html.HiddenFor(m => m.medics_Used.ID)
                                                                @Html.HiddenFor(m => m.neurological_System.ID)
                                                                @Html.HiddenFor(m => m.obstetrik_History.ID)
                                                                @Html.HiddenFor(m => m.other_Endocrine_System.ID)
                                                                @Html.HiddenFor(m => m.other_Systems.ID)
                                                                @Html.HiddenFor(m => m.physical_Examination.ID)
                                                                @Html.HiddenFor(m => m.tumor_Markers.ID)
                                                                @Html.HiddenFor(m => m.urine.ID)
                                                                @Html.HiddenFor(m => m.urine_Spot.ID)

                                                            </div>
                                                        </div>



                                                        <div class="col-md-2">
                                                            <div class="form-group">
                                                                <label for="field-1" class="control-label">
                                                                    TCK
                                                                    No
                                                                </label>
                                                                @Html.TextBoxFor(m => m.patient_Info.FileNo, new { @class = "form-control" })
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="row">
                                                        <div class="col-md-2">
                                                            <div class="form-group">
                                                                <label for="field-2" class="control-label">
                                                                    Dosya
                                                                    No
                                                                </label>
                                                                @Html.TextBoxFor(m => m.patient_Info.IDNo, new { @class = "form-control" })

                                                            </div>
                                                        </div>
                                                        <div class="col-md-2">
                                                            <div class="form-group">
                                                                <label for="field-3" class="control-label">Adı</label>
                                                                @Html.TextBoxFor(m => m.patient_Info.Name, new { @class = "form-control" })
                                                            </div>
                                                        </div>
                                                        <div class="col-md-2">
                                                            <div class="form-group">
                                                                <label for="field-4" class="control-label">Soyadı</label>
                                                                @Html.TextBoxFor(m => m.patient_Info.Surname, new { @class = "form-control" })
                                                            </div>
                                                        </div>


                                                        <div class="col-md-2">
                                                            <div class="form-group">
                                                                <label for="field-8" class="control-label">Cinsiyet</label>
                                                                @Html.DropDownListFor(m => m.patient_Info.Gender, new List<SelectListItem>
                                                                   {
                                                                     new SelectListItem{Text = "Kadın" , Value = "Kadin"},
                                                                     new SelectListItem{Text = "Erkek" , Value = "Erkek"},
                                                                     }, "Seçiniz", new { @class = "btn btn-primary waves-effect waves-light dropdown-toggle" })

                                                            </div>
                                                        </div>

                                                        <div class="col-md-2">
                                                            <div class="form-group">
                                                                <label for="field-6" class="control-label">
                                                                    Doğum
                                                                    Yeri
                                                                </label>
                                                                @Html.TextBoxFor(m => m.patient_Info.BirthPlace, new { @class = "form-control" })
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="row">
                                                        <div class="col-md-2">
                                                            <div class="form-group">
                                                                <label for="field-7" class="control-label">
                                                                    Doğum
                                                                    Tarihi
                                                                </label>
                                                                @Html.TextBoxFor(m => m.patient_Info.BirthDate, new { @class = "form-control", type = "date", name = "date" })
                                                            </div>
                                                        </div>


                                                        <div class="col-md-2">
                                                            <div class="form-group">
                                                                <label for="field-8" class="control-label">
                                                                    Medeni
                                                                    Durum
                                                                </label>
                                                                @Html.DropDownListFor(m => m.patient_Info.MaritalStatus, new List<SelectListItem>
                                                                   {
                                                                     new SelectListItem{Text = "Evli" , Value = "Evli"},
                                                                     new SelectListItem{Text = "Bekar" , Value = "Bekar"},
                                                                      new SelectListItem{Text = "Dul" , Value = "Dul"},
                                                                      new SelectListItem{Text = "Ayrılmış" , Value = "Ayrılmis"},
                                                                     }, "Seçiniz", new { @class = "btn btn-primary waves-effect waves-light dropdown-toggle" })
                                                            </div>
                                                        </div>

                                                        <div class="col-md-2">
                                                            <div class="form-group">
                                                                <label for="field-8" class="control-label">
                                                                    Eğitim
                                                                    Durumu
                                                                </label>
                                                                @Html.TextBoxFor(m => m.patient_Info.EduStatus, new { @class = "form-control" })
                                                            </div>
                                                        </div>

                                                        <div class="col-md-2">
                                                            <div class="form-group">
                                                                <label for="field-8" class="control-label">
                                                                    Aldığı
                                                                    Maaş
                                                                </label>
                                                                @Html.DropDownListFor(m => m.patient_Info.Salary, new List<SelectListItem>
                                                                   {
                                                                     new SelectListItem{Text = "AÜ" , Value = "AU"},
                                                                     new SelectListItem{Text = "AÜx2" , Value = "AUx2"},
                                                                      new SelectListItem{Text = "AÜx3-4" , Value = "AUx3-4"},
                                                                      new SelectListItem{Text = "AÜx4" , Value = "AUx4"},
                                                                     }, "Seçiniz", new { @class = "btn btn-primary waves-effect waves-light dropdown-toggle" })
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <h4 class="modal-title">Diyabet Bilgileri</h4>
                                                    <div class="row">
                                                        <div class="col-md-2">
                                                            <div class="form-group">
                                                                <label for="field-7" class="control-label">
                                                                    Diyabet
                                                                    Başlangıç
                                                                    Tarihi
                                                                </label>
                                                                @Html.TextBoxFor(m => m.diabetes_Info.StartingDate, new { @class = "form-control", type = "date", name = "date" })
                                                            </div>
                                                        </div>

                                                        <div class="col-md-2">
                                                            <div class="form-group">
                                                                <label for="field-8" class="control-label">
                                                                    Diyabetin
                                                                    Başlangıç
                                                                    Şekli
                                                                </label>
                                                                @Html.DropDownListFor(m => m.diabetes_Info.InitialShape, new List<SelectListItem>
                                                                 {
                                                                   new SelectListItem{Text = "AKUT HG" , Value = "AKUT HG"},
                                                                   new SelectListItem{Text = "DKA/NKK" , Value = "DKA/NKK"},
                                                                    new SelectListItem{Text = "Tesadüfi" , Value = "Tesadüfi"},
                                                                  }, "Seçiniz", new { @class = "btn btn-primary waves-effect waves-light dropdown-toggle" })

                                                            </div>
                                                        </div>

                                                        <div class="col-md-2">
                                                            <div class="form-group">
                                                                <label for="field-8" class="control-label">
                                                                    Başvuru
                                                                    Nedeni
                                                                </label>
                                                                @Html.DropDownListFor(m => m.diabetes_Info.Reason, new List<SelectListItem>
                                                                {
                                                                   new SelectListItem{Text = "Yeni Tanılı DM" , Value = "Yeni Tanılı DM"},
                                                                   new SelectListItem{Text = "Rutin Kontrol" , Value = "Rutin Kontrol"},
                                                                   new SelectListItem{Text = "Kontrolsüz DM" , Value = "Kontrolsuz DM"},
                                                                   new SelectListItem{Text = "A1c Yüksekliği" , Value = "A1c Yüksekligi"},
                                                                   new SelectListItem{Text = "Başka yerde takipli ama glisemi regülasyon bozuk" , Value = "Baska yerde takipli ama glisemi regulasyon bozuk"},
                                                                   new SelectListItem{Text = "Komplikasyonlu DM" , Value = "Komplikasyonlu DM"},
                                                                   new SelectListItem{Text = "Diğer(Açıklama)" , Value = "Diger(Aciklama)"},
                                                                 }, "Seçiniz", new { @class = "btn btn-primary waves-effect waves-light dropdown-toggle" })

                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="form-group row">
                                                        <label class="col-8 col-form-label">Yakınması</label>
                                                        <div class="col-10">
                                                            @Html.TextAreaFor(m => m.diabetes_Info.Gripe, new { @class = "form-control" })
                                                        </div>
                                                    </div>

                                                    <div class="form-group row">
                                                        <label class="col-8 col-form-label">Hikayesi</label>
                                                        <div class="col-10">
                                                            @Html.TextAreaFor(m => m.diabetes_Info.Story, new { @class = "form-control" })
                                                        </div>
                                                    </div>
                                                    <h4 class="modal-title">Başvuruda Kullandığı İlaçlar</h4>


                                                    <div class="form-group row">
                                                        <label class="col-8 col-form-label">Diyabet İlaçları</label>
                                                        <div class="col-10">
                                                            @Html.TextAreaFor(m => m.medics_Used.DiabetesMedic, new { @class = "form-control" })
                                                        </div>
                                                    </div>
                                                    <div class="form-group row">
                                                        <label class="col-8 col-form-label">Diğer İlaçlar</label>
                                                        <div class="col-10">
                                                            @Html.TextAreaFor(m => m.medics_Used.OtherMedic, new { @class = "form-control" })
                                                        </div>
                                                    </div>


                                                    <h4 class="modal-title">Alışkanlıklar</h4>

                                                    <div class="row">
                                                        <div class="col-md-2">
